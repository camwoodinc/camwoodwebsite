{
  "version": 2,
  "builds": [
    {
      "src": "camwood-react/index.js",
      "use": "@vercel/vite",
      "config": { "includeBuildGlobal": true }
    },
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    
    {
      "src": "/(.*)",
      "dest": "camwood-react/index.html" 
    },
    
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    }
  ]
}